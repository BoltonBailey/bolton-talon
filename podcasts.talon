os: mac
app: Podcasts
-

skip forward:
    user.menu_select('Controls|Skip 30 sec')

skip back:
    key(cmd-left)

play podcast:
    key(space)

normal speed:
    key(alt-1)

one point two five speed:
    key(alt-2)

one point five speed:
    key(alt-3)

one point seven five speed:
    key(alt-4)

two times speed:
    key(alt-5)